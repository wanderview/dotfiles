fixdel

au BufNewFile,BufRead *.jsm set filetype=javascript

set softtabstop=2
set shiftwidth=2
set expandtab

set autoindent
set backspace=indent

set title
set titleold=

set ruler

set nowrap
set list
set listchars=tab:>-,trail:-,extends:>,precedes:<

set wildmenu
set wildmode=longest:full
set wildignore=*.o,*.class

set ttyscroll=1

set background=dark
colorscheme desert
syntax on
